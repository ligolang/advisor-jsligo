export type storage = int;
export type parameter = int;
export type return_ = [list<operation>, storage];

@entry
const increment = (param: int, store: storage) : return_ => {
    return [list([]) as list<operation>, store + param]
};

@entry
const decrement = (param: int, store: storage) : return_ => {
    return [list([]) as list<operation>, store - param]
};

@view 
export const indice_value = (_ : unit, store : storage) : int => { return store };
