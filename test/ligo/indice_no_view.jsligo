export type indiceStorage = int;

export type indiceFullReturn = [list<operation>, indiceStorage];

@entry
const increment = (param: int, store: indiceStorage) : indiceFullReturn => {
    return [list([]) as list<operation>, store + param]
};

@entry
const decrement = (param: int, store: indiceStorage) : indiceFullReturn => {
    return [list([]) as list<operation>, store - param]
};